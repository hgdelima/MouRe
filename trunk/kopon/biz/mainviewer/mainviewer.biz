#biz
	mainviewer

#node
	tabbank tabbar
	multipageviewer pages

#start
	init

#message
	user->logedin=onLogedin
	user->logedout=onLogedout

#frame
	frm(float:left; width:600;)

#phpfunction

	function init(){
		$tab=array("Main","Previous","How");
		if(osIsAdmin()){
			$tab[]="CPanel";
		}
		$user=osBackUser();
		if($user['UID']!=-1){
			$tab[]="MyAcc";
		}
		#node->tabbar->bookContent($tab);
	}

	function onLogedin($info){
		#fun->init();
		_bookFrame(#frame->frm);
	}

	function onLogedout($info){
		#fun->init();
		_bookFrame(#frame->frm);
	}

	function frm(){
		$tab=#node->tabbar->_backFrame();
		$pages=#node->pages->_backFrame();
		$html=<PHTML>
			$tab <br> $pages
		</PHTML>;
		return $html;
	}
